    "keywords": [ "cache", "symfony2", "service", "container"],
    "homepage": "",
    "license": "MIT",
    "authors": [
    {
        "name": "Steven Lewis",
        "email": "steven.lewis@epiphanysolutions.co.uk"
    }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "2.3.*",
        "symfony/monolog-bundle": "2.4.*"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.4.*"
